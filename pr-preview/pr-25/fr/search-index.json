 when ordering is irrelevant. Keep list items parallel. Start numbered list items with imperative words. Introduce lists and tables appropriately. Create great opening sentences that establish a paragraph's central point. Focus each paragraph on a single topic. Determine what your audience needs to learn. Fit documentation to your audience. Establish your document's key points at the start of the document.","s":"Style Guide","u":"/docu-notion-sample/pr-preview/pr-25/fr/style-guide","h":"","p":11}],"index":{"version":"2.3.9","fields":["t"],"fieldVectors":[["t/8",[0,1.562,1,1.562,2,0.748,3,1.562]],["t/10",[4,1.585,5,1.585,6,1.585]],["t/12",[2,0.268,7,0.889,8,0.559,9,0.559,10,0.559,11,0.559,12,0.889,13,0.559,14,0.559,15,0.559,16,0.559,17,0.889,18,0.559,19,0.889,20,1.105,21,0.559,22,0.559,23,0.559,24,0.889,25,1.259,26,0.889,27,0.559,28,0.889,29,0.889,30,0.889,31,0.559,32,0.889,33,0.559,34,0.559,35,1.259,36,1.105,37,0.559,38,0.559,39,0.559,40,0.559,41,0.559,42,0.559,43,0.559,44,0.559,45,0.559,46,0.559,47,0.559,48,0.559,49,0.559,50,0.559,51,0.559,52,0.889,53,0.889,54,0.889,55,0.559,56,0.559,57,0.559,58,1.462,59,0.559,60,0.559,61,0.889,62,0.559,63,0.559,64,0.559,65,1.105,66,0.559,67,0.559,68,0.559,69,0.889,70,0.889,71,0.559,72,0.559,73,0.559,74,0.889,75,0.559,76,0.889,77,0.559,78,0.559,79,0.559,80,0.559,81,0.559,82,0.559,83,0.559,84,0.889,85,0.559,86,0.559,87,0.559,88,0.559,89,0.889,90,0.559,91,0.559,92,0.559,93,0.559]]],"invertedIndex":[["3",{"_index":33,"t":{"12":{"position":[[358,1]]}}}],["6",{"_index":34,"t":{"12":{"position":[[360,1]]}}}],["abbrevi",{"_index":63,"t":{"12":{"position":[[636,13]]}}}],["activ",{"_index":24,"t":{"12":{"position":[[194,6],[290,6]]}}}],["ambigu",{"_index":44,"t":{"12":{"position":[[471,9]]}}}],["appear",{"_index":68,"t":{"12":{"position":[[696,11]]}}}],["appropri",{"_index":80,"t":{"12":{"position":[[906,14]]}}}],["approv",{"_index":29,"t":{"12":{"position":[[264,8],[317,8]]}}}],["audienc",{"_index":89,"t":{"12":{"position":[[1056,8],[1107,